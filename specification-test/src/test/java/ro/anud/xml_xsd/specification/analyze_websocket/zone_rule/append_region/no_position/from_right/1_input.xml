<world_step
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="../../../../../../../../../../../../../world_step.xsd">
  <world_metadata>
    <elapsed_time value="0"/>
    <stepDuration value="0"/>
    <counter value="0"/>
    <randomization_table>
      <entry value="1"/>
    </randomization_table>
  </world_metadata>
  <rule_group id="rule_group_id">
    <region_rule>
      <entry id="region_rule">
        <limit>
          <width initial="10"/>
          <height initial="10"/>
        </limit>
        <portal>
          <from side="right">
            <start initial="0"/>
            <width initial="1"/>
          </from>
          <to>
            <region region_rule_ref="region_rule" side="left">
              <start initial="0"/>
              <width initial="1"/>
            </region>
          </to>
        </portal>
      </entry>
    </region_rule>
    <zone_rule>
      <entry id="zone_rule">
        <starting_region region_rule_ref="region_rule"/>
      </entry>
    </zone_rule>
  </rule_group>
  <data>
    <location/>
    <people/>
    <zone_list>
      <zone id="zone_id">
        <region id="region_id">
          <rule rule_id_ref="region_rule"/>
          <position x="0" y="0"/>
          <limit height="10" width="10"/>
          <portals>
            <portal id="portal_id">
              <from side="right" start="0" end="1"/>
            </portal>
          </portals>
        </region>
      </zone>
    </zone_list>
  </data>
  <actions>
    <region.append zone_id_ref="zone_id" region_id_ref="region_id"/>
  </actions>
</world_step>