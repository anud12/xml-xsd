<world_step
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="../../../../../../../../../../../world_step.xsd">
  <world_metadata>
    <elapsed_time value="0"/>
    <stepDuration value="0"/>
    <counter value="0"/>
    <randomization_table>
      <entry value="2"/>
    </randomization_table>
  </world_metadata>
  <rule_group id="rule_group_id">
    <property_rule>
      <entry id="property" units="">
        <person_default initial="1"/>
      </entry>
    </property_rule>

    <location_graph_rule id="location_graph_rule">
      <setup>
        <starting_node node_rule_ref="node_rule"/>
      </setup>
      <node_rule id="node_rule"/>
    </location_graph_rule>
  </rule_group>
  <data>
    <location>
      <location_graph id="location_graph">
        <rule location_graph_rule_ref="location_graph_rule"/>
        <node node_rule_ref="node_rule" id="node_id"/>
      </location_graph>
    </location>
  </data>
  <actions>
    <person.create>
      <node_graph__selection>
        <has__node_graph_id node_graph_id_ref="node_id"/>
      </node_graph__selection>
      <person__selection>
        <min initial="1"/>
      </person__selection>
    </person.create>
  </actions>
</world_step>