<world_step
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="../../../../../../../../../../../../../../../../world_step.xsd">
  <world_metadata>
    <elapsed_time value="0"/>
    <stepDuration value="0"/>
    <counter value="0"/>
    <randomization_table>
      <entry value="1"/>
    </randomization_table>
  </world_metadata>
  <rule_group id="rule_group_id">
    <portal_rule>
      <entry id="portal_rule">
        <limit width="2"/>
        <to>
          <region region_rule_ref="region_rule"/>
        </to>
      </entry>
    </portal_rule>
    <region_rule>
      <entry id="region_rule">
        <limit>
          <width initial="10"/>
          <height initial="6"/>
        </limit>
        <portals>
          <portal side="top" portal_rule_ref="portal_rule">
            <start initial="3"/>
          </portal>
        </portals>
      </entry>
    </region_rule>
    <zone_rule>
      <entry id="zone_rule">
        <starting_region region_rule_ref="region_rule"/>
      </entry>
    </zone_rule>
  </rule_group>
  <data>
    <location/>
    <zone_list>
      <zone id="zone_id">
        <region id="region_id">
          <rule rule_id_ref="region_rule"/>
          <position x="100" y="1000" rotation="counterclockwise"/>
          <limit height="6" width="10"/>
          <available_portals>
            <portal id="available_portal_id" portal_rule_ref="portal_rule" side="top" start="3"/>
          </available_portals>
        </region>
      </zone>
    </zone_list>
  </data>
  <actions>
    <region.appendNew zone_id_ref="zone_id" region_id_ref="region_id" portal_id_ref="available_portal_id"/>
  </actions>
</world_step>