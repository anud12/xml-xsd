<world_step
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="../../../../../../../../../../../world_step.xsd">
  <world_metadata>
    <elapsed_time value="0"/>
    <stepDuration value="0"/>
    <counter value="0"/>
    <randomization_table>
      <entry value="2"/>
    </randomization_table>
  </world_metadata>
  <rule_group id="rule_group_id">
    <location_graph_rule id="location_graph_rule">
      <setup>
        <starting_node node_rule_ref="node_rule"/>
      </setup>
      <node_rule id="node_rule">
        <link_group_list>
          <link_group id="link_group" angle="0">
            <to_option node_rule_ref="node_rule" distance="0" adjacent_depth_limit="0"/>
          </link_group>
        </link_group_list>
      </node_rule>
    </location_graph_rule>
  </rule_group>
  <data>
    <location>
      <location_graph id="location_graph">
        <rule location_graph_rule_ref="location_graph_rule"/>
        <node node_rule_ref="node_rule" id="node_id">
          <position x="0" y="0"/>
        </node>
      </location_graph>
    </location>
  </data>
  <actions>
    <location_graph.node.create_adjacent location_graph_id_ref="location_graph" node_id_ref="node_id"/>
  </actions>
</world_step>