    </people>
    <zone id="dd" name="dd">
      <region id="12" name="123">
        <rule rule_id_ref="demo"/>
        <position x="123" y="123"/>
        <limit height="10" width="10"/>
        <portals>
          <portal id="">
            <from side="bottom" position="2" width="10"/>
            <to zone_ref="" region_ref="" side="bottom" position="2"/>
          </portal>
        </portals>
      </region>
    </zone>
  </data>
  <actions>
    <by person_ref="1">
      <do action_rule_ref="action_rule" person_ref="1" />
    </by>
  </actions>
