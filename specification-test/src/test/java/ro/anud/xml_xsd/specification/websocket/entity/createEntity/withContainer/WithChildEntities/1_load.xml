<world_step
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="../../../../../../../../../../../../../world_step.xsd">
  <world_metadata>
    <elapsed_time value="0"/>
    <stepDuration value="0"/>
    <counter value="0"/>
    <randomization_table>
      <entry value="2"/>
    </randomization_table>
  </world_metadata>
  <rule_group id="rule_group_id">
    <container_rule>
      <entry name="container_rule">
        <allowed_entity entity_rule_ref="child_entity">
          <max initial="1"/>
          <min initial="1"/>
        </allowed_entity>
      </entry>
    </container_rule>
    <entity_rule>
      <entry name="child_entity"/>
      <entry name="entity_rule">
        <containers>
          <container container_rule_ref="container_rule"/>
        </containers>
      </entry>
    </entity_rule>
  </rule_group>
  <actions>
    <entity.create entity_rule_ref="entity_rule"/>
  </actions>
</world_step>