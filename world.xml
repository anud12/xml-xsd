    <action_rule>
      <person_to_person id="talk">
        <max_range>
          <operation initial="1"/>
        </max_range>
        <test>
          <value target="self">
            <operation initial="1"/>
          </value>
          <expected target="target">
            <operation initial="1"/>
          </expected>
        </test>
      </person_to_person>
      <person_to_person id="meleeAttack">
        <max_range>
          <operation initial="2"/>
        </max_range>
        <test>
          <value target="self">
            <operation initial="3"/>
          </value>
          <expected target="target">
            <operation initial="3"/>
          </expected>
        </test>
        <property_mutation property_rule_ref="health" on="target">
          <from participant="self">
            <operation initial="0">
              <and do="add_dice" value="6"/>
              <and do="add" value="0">
                <add_property property_rule_ref="strength"/>
                <and do="add" value="-10"/>
                <and do="divide" value="2"/>
              </and>
              <and do="multiply" value="-1"/>
            </operation>
          </from>
        </property_mutation>
      </person_to_person>
    </action_rule>
