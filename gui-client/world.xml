  <rule_group id="rule_group_id">
    <property_rule>
      <entry id="health" units="points" />
      <entry id="strength" units="points">
        <person_default initial="10" />
      </entry>
    </property_rule>
    <classification_rule>
      <entry id="alive">
        <property property_rule_ref="health" is="greaterThan">
          <operation initial="0" />
        </property>
      </entry>
      <entry id="dead">
        <property property_rule_ref="health" is="lessThanOrEqual">
          <operation initial="0" />
        </property>
      </entry>
    </classification_rule>
    <action_rule>
      <person_to_person id="meleeAttack">
        <test>
          <value target="self">
            <operation initial="0"></operation>
          </value>
          <expected target="self">
            <operation initial="0"></operation>
          </expected>
        </test>
        <property_mutation property_rule_ref="health" on="target">
          <from participant="self">
            <operation initial="0">
              <add_property property_rule_ref="strength" />
              <and do="divide" value="2" />
              <and do="multiply" value="-1" />
            </operation>
          </from>
        </property_mutation>
      </person_to_person>
    </action_rule>
    <location_graph_rule id="location_graph_rule_id">
      <setup>
        <starting_node node_rule_ref="farm" />
        <necessary_node node_rule_ref="city" min="8" />
      </setup>
      <node_rule id="city">
        <link_group id="all" angle="0" angleMax="360" limit="100">
          <to_option node_rule_ref="farm" distance="1" maxDistance="2" adjacent_depth_limit="5" />
        </link_group>
      </node_rule>
      <node_rule id="farm">
        <link_group id="all" angle="0" angleMax="360" limit="3">
          <to_option node_rule_ref="city" distance="3" maxDistance="5" adjacent_depth_limit="5" />
          <to_option node_rule_ref="farm" distance="3" maxDistance="5" adjacent_depth_limit="5" />
          <to_option node_rule_ref="farm" distance="3" maxDistance="5" adjacent_depth_limit="5" />
          <to_option node_rule_ref="forest" distance="5" maxDistance="6" adjacent_depth_limit="5">
            <distance_to_progress_multiplier initial="1">
              <and do="multiply_dice" value="3" />
            </distance_to_progress_multiplier>
          </to_option>
          <to_option node_rule_ref="forest" distance="5" maxDistance="6" adjacent_depth_limit="5">
            <distance_to_progress_multiplier initial="1">
              <and do="multiply_dice" value="3" />
            </distance_to_progress_multiplier>
          </to_option>
          <to_option node_rule_ref="forest" distance="5" maxDistance="6" adjacent_depth_limit="5">
            <distance_to_progress_multiplier initial="1">
              <and do="multiply_dice" value="3" />
            </distance_to_progress_multiplier>
          </to_option>
          <to_option node_rule_ref="forest" distance="5" maxDistance="6" adjacent_depth_limit="5">
            <distance_to_progress_multiplier initial="1">
              <and do="multiply_dice" value="3" />
            </distance_to_progress_multiplier>
          </to_option>
          <to_option node_rule_ref="forest" distance="5" maxDistance="6" adjacent_depth_limit="5">
            <distance_to_progress_multiplier initial="1">
              <and do="multiply_dice" value="3" />
            </distance_to_progress_multiplier>
          </to_option>
        </link_group>
      </node_rule>
      <node_rule id="forest">
        <link_group id="all" angle="0" angleMax="360" limit="3">
          <to_option node_rule_ref="farm" distance="1" maxDistance="20" adjacent_depth_limit="5" />
          <to_option node_rule_ref="forest" distance="5" maxDistance="6" adjacent_depth_limit="5">
            <distance_to_progress_multiplier initial="1">
              <and do="multiply_dice" value="3" />
            </distance_to_progress_multiplier>
          </to_option>
          <to_option node_rule_ref="forest" distance="5" maxDistance="6" adjacent_depth_limit="5">
            <distance_to_progress_multiplier initial="1">
              <and do="multiply_dice" value="3" />
            </distance_to_progress_multiplier>
