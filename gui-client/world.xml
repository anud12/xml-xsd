          <to_option node_rule_ref="forest" distance="5" maxDistance="6" adjacent_depth_limit="5">
            <distance_to_progress_multiplier initial="1">
              <and do="multiply_dice" value="3" />
            </distance_to_progress_multiplier>
          </to_option>
        </link_group>
      </node_rule>
    </location_graph_rule>
